{
  "version": 2,
  "timeout": "9000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "sales-guide",
  "plugin": {
    "pattern": ".so",
    "folder": "./"
  },
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": [
        "*"
      ],
      "expose_headers": [
        "Content-Length",
        "*"
      ],
      "max_age": "12h",
      "allow_methods": [
        "GET",
        "HEAD",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS",
        "PATCH"
      ],
      "allow_headers": [
        "*"
      ],
      "allow_credentials": true
    },
    "github_com/devopsfaith/krakend-gologging": {
      "level": "INFO",
      "prefix": "[KRAKEND]",
      "syslog": false,
      "stdout": true,
      "format": "default"
    }
  },
  "endpoints": [
    {
      "endpoint": "/sales-guide/api/v1/companies",
      "method": "POST",
      "output_encoding": "json",
      "extra_config": {
        "github.com/devopsfaith/krakend-ratelimit/juju/router": {
          "maxRate": 15,
          "clientMaxRate": 5,
          "strategy": "ip",
          "key": ""
        },
        "github_com/devopsfaith/krakend/transport/http/server/handler": {
          "name": "grpc-post",
          "attachuserid": "rsc"
        }
      },
      "concurrent_calls": 1,
      "headers_to_pass": [
        "Content-Type",
        "Content-Length",
        "Host",
        "User-Agent",
        "Accept",
        "Accept-Encoding",
        "x-timezone",
        "x-timezone-offset",
        "x-apikey",
        "Authorization",
        "x-cid",
        "X-Friend-User"
      ],
      "backend": [
        {
          "url_pattern": "/api/v1/companies",
          "encoding": "json",
          "sd": "static",
          "method": "POST",
          "extra_config": {},
          "host": [
            "http://localhost:5050"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/sales-guide/api/v1/companies/{companyID}",
      "method": "GET",
      "output_encoding": "json",
      "extra_config": {
        "github.com/devopsfaith/krakend-ratelimit/juju/router": {
          "maxRate": 15,
          "clientMaxRate": 5,
          "strategy": "header",
          "key": "Authorization"
        },
        "github.com/devopsfaith/krakend-jose/validator": {
          "alg": "RS256",
          "jwk-url": "https://drprado2.us.auth0.com/.well-known/jwks.json",
          "audience": [
            "http://localhost:8000"
          ]
        }
      },
      "concurrent_calls": 1,
      "headers_to_pass": [
        "Content-Type",
        "Content-Length",
        "Host",
        "User-Agent",
        "Accept",
        "Accept-Encoding",
        "x-timezone",
        "x-timezone-offset",
        "x-apikey",
        "Authorization",
        "x-cid"
      ],
      "backend": [
        {
          "url_pattern": "/api/v1/companies/{companyID}",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "extra_config": {},
          "host": [
            "http://localhost:5050"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}